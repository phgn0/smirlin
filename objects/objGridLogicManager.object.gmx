            <kind>1</kind>
            <string>/// generate grass, tree positions

var trees_grid;
grass_coords = ds_grid_create(MAP_GRID_SIZE, MAP_GRID_SIZE);
for (var i = 0; i &lt; MAP_GRID_SIZE; i++) {
    for (var j = 0; j &lt; MAP_GRID_SIZE; j++) {
        grass_coords[# i, j] = scrGridViewController_translateGridToRealCoords(i, j);
    
        trees_grid[i, j] = ds_list_create();
    }
}

for (var t = 0; t &lt; TREE_AMOUNT; t++) {
    // pick random cell for the tree
    var cell_x = irandom(MAP_GRID_SIZE - 1);
    var cell_y = irandom(MAP_GRID_SIZE - 1);
       
    // create a tree at a random postion in that cell
    var tree_grid_x = cell_x + random(1);
    var tree_grid_y = cell_y + random(1);
    
    ds_list_add(trees_grid[cell_x, cell_y], 
            scrGridViewController_translateGridToRealCoords(tree_grid_x, tree_grid_y, true));
}

trees = trees_grid;
