            <string>/// set up buildings list

buildings_configs = ds_list_create();

var buildings_map = global.external_config[? "buildings"];

// list of all building configs
var keys_list = buildings_map[? "directory_keys"];
var size = ds_list_size(keys_list);

for (var i = 0; i &lt; size; i++;) {
    var key = keys_list[| i];
    
    //TODO check if requirements satisfied
    
    ds_list_add(buildings_configs, buildings_map[? key]);
}

// the index of the selected building
selected_building_slot = undefined;
