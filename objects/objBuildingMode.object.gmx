        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// move preview object if mouse moved to new cell

if (selected_building_slot != undefined) {
    var pos = scrGridViewController_getCurrentCell();

    if (not (pos[0] == preview_instance.grid_position[0])
            or not (pos[1] == preview_instance.grid_position[1])) {
        // we moved to new cell
        var config = buildings_configs[| selected_building_slot];
        var size = config[? "size"];
        
        preview_instance.grid_position = pos;
        
        if (scrGridLogicManager_isLegalPlacement(pos[0], pos[1],
                size[| 0], size[| 1])) {
            // move preview
            
            scrBuildingMode_showPreview(true);
            
            scrGridViewController_changeInstancePosition(
                pos[0], pos[1], preview_instance);
        } else {
            // illegal placement

            scrBuildingMode_showPreview(false);
        }
    } 
}
</string>
